@{
    Layout = "~/Views/Shared/_FrameLayout.cshtml";
    ViewBag.Title = "";
}
<div class="easyui-layout" style="width:100%;height:100%;">
    <div data-options="region:'north',title:'North Title',split:false,noheader:true" style="overflow:hidden;height:150px;">
        @{Html.RenderAction("Header");}
    </div>
    <div data-options="region:'south',title:'South Title',split:false,noheader:true" style="overflow:hidden;height:50px;">
        @{Html.RenderAction("Footer");}
    </div>
    @*<div data-options="region:'east',title:'East',split:true" style="width:100px;"></div>*@
    <div data-options="region:'west',title:'West',split:true,minWidth:150,maxWidth:350" style="width:15%"></div>
    <div data-options="region:'center',title:'center title',noheader:true" style="overflow:hidden;">
        <div id="tabs" class="easyui-tabs" fit="true" border="false">
        </div>
    </div>
</div>

